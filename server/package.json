{
  "name": "starhackit-api",
  "description": "The starhackit backend",
  "version": "0.0.1",
  "private": false,
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:FredericHeem/starhackit.git"
  },
  "license": "NLPL",
  "scripts": {
    "devlabinstall": "lab install",
    "prebuild": "npm run lint",
    "build": "gulp build:production",
    "start": "gulp",
    "pretest": "npm run build",
    "test": "npm run coverage",
    "mocha": "mocha ./src/**/test*.js ./test/**/test*.js",
    "coverage": "istanbul  cover -x 'src/**/test*.js' -x 'test/**/test*.js' _mocha --report text --report html --report lcov ./src/**/test*.js ./test/**/*.js",
    "coveralls": "coveralls < coverage/lcov.info",
    "codecov": "./node_modules/codecov.io/bin/codecov.io.js < coverage/lcov.info",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info",
    "lint": "eslint src",
    "migrate": "sequelize db:migrate",
    "postinstall": "npm run build",
    "nodemon": "nodemon",
    "run": "node build/index.js",
    "clean": "gulp clean"
  },
  "dependencies": {
    "babel": "5.8.23",
