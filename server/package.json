    "gulp-rimraf": "0.2.0",
    "gulp-util": "3.0.6",
    "jsonschema": "1.0.2",
    "koa": "2.0.0-alpha.3",
    "koa-66": "0.7.3",
    "koa-bodyparser": "3.0.0",
    "koa-convert": "1.2.0",
